{
  "name": "andrew72ru/yii2-pen",
  "description": "Content editor like Medium.com for yii-2 based on https://github.com/sofish/pen",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","content-editable","WYSIWYG","editor"],
  "license": "MIT",
  "authors": [
    {
      "name": "andrew72ru",
      "email": "andrew72ru@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "bower-asset/pen": "~0.0.2"
  },
  "autoload": {
    "psr-4": {
      "andrew72ru\\pen\\": "src"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "bower-asset-library": "vendor/bower"
    }
  }
}
